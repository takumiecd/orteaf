# Doxygen configuration for ORTEAF test documentation (English - default).

PROJECT_NAME           = "ORTEAF Tests"
PROJECT_BRIEF          = "Test suites and specifications for ORTEAF"

OUTPUT_LANGUAGE        = English
OUTPUT_DIRECTORY       = docs/api-tests
GENERATE_HTML          = YES
GENERATE_LATEX         = NO

INPUT                  = tests
FILE_PATTERNS          = *.cpp \
                         *.cc \
                         *.cxx \
                         *.md
RECURSIVE              = YES

EXTRACT_PRIVATE        = NO
EXTRACT_STATIC         = NO
EXTRACT_LOCAL_METHODS  = NO
EXTRACT_ALL            = NO

ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
EXPAND_ONLY_PREDEF     = NO
PREDEFINED             = ORTEAF_STATS_LEVEL_GLOBAL_VALUE=8 \
                         ORTEAF_STATS_LEVEL_CPU_VALUE=8 \
                         ORTEAF_STATS_LEVEL_CUDA_VALUE=8 \
                         ORTEAF_STATS_LEVEL_MPS_VALUE=8 \
                         ORTEAF_STATS_LEVEL_CORE_VALUE=8

GENERATE_TREEVIEW      = YES
FULL_PATH_NAMES        = NO
JAVADOC_AUTOBRIEF      = YES
MARKDOWN_SUPPORT       = YES
INLINE_SOURCES         = NO

WARN_IF_UNDOCUMENTED   = NO
QUIET                  = NO
WARNINGS               = YES

SEARCHENGINE           = NO
